{
  "presets": [
    ["@babel/preset-env", {
      "modules": false,
      "targets": {
        "chrome": "58",
        "ie": "11"
      }
    }]
  ],
  "plugins": [
    "@babel/plugin-transform-flow-strip-types",
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-syntax-dynamic-import"
  ]
}
